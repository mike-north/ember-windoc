{
  "name": "ember-windoc",
  "version": "0.2.0",
  "description": "Window and document events (scroll, resize, move, etc...) the Ember.js way",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@commitlint/cli": "7.6.0",
    "@commitlint/config-conventional": "7.6.0",
    "@commitlint/travis-cli": "7.6.0",
    "@ember/optional-features": "0.7.0",
    "@mike-north/js-lib-renovate-config": "1.1.5",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli-app-version": "3.2.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-htmlbars-inline-precompile": "3.0.2",
    "ember-cli-htmlbars": "5.7.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-template-lint": "1.0.0-beta.3",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "3.0.0",
    "ember-cli": "3.27.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-resolver": "8.0.3",
    "ember-source-channel-url": "1.2.0",
    "ember-source": "3.28.2",
    "ember-sublime": "0.0.9",
    "ember-try": "1.2.1",
    "eslint-plugin-ember": "10.5.7",
    "eslint-plugin-node": "11.1.0",
    "husky": "1.2.1",
    "loader.js": "4.7.0",
    "semantic-release": "15.12.5",
    "qunit-dom": "2.0.0"
  },
  "keywords": [
    "ember-addon",
    "window",
    "scroll",
    "resize",
    "event",
    "document",
    "fastboot"
  ],
  "repository": "https://github.com/mike-north/ember-windoc",
  "license": "MIT",
  "author": "Mike North (http://mike.works) <michael.l.north@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "ember-cli-babel": "^7.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoUrl": "https://ember-windoc.herokuapp.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
